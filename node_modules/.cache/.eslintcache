hedTodos = [];\r\n\r\n  if (!searchValue.length >= 1){\r\n    searchedTodos = todos;\r\n  }else{\r\n    searchedTodos = todos.filter(todo => {\r\n      const todoText = todo.text.toLowerCase();\r\n      const searchText = searchValue.toLowerCase();\r\n      return todoText.includes(searchText);\r\n    });\r\n    TodoCreate();\r\n  }\r\n\r\n\r\nfunction TodoCreate(){\r\n  if (searchedTodos.length <= 0){\r\n    createButtton.classList.remove(\"Disable-button\");\r\n  }else{\r\n    createButtton.classList.add(\"Disable-button\");\r\n  };\r\n}\r\n\r\n\r\n\treturn(\r\n\t\t<React.Fragment>\r\n\t\t<section className=\"TodoBox\">\r\n    <section className=\"TodoBox--marc\"></section>\r\n        <TodoCounter\r\n          total={totalTodos}\r\n          completed={completedTodos}\r\n        />\r\n        <TodoSearch\r\n          searchValue={searchValue}\r\n          setSearchValue={setSearchValue}\r\n\r\n\r\n        />\r\n        <CreateTodoButton/>\r\n        <TodoList>\r\n          {searchedTodos.map(todo => (\r\n            <TodoItem key={todo.text} text={todo.text} value={todo.value} progress={todo.progress} completed={todo.completed} />\r\n            ))}\r\n        </TodoList>\r\n    \t</section>\r\n    \t</React.Fragment>\r\n\t);\r\n}\r\n\r\nexport { TodoPop };",{"ruleId":"45","severity":1,"message":"46","line":16,"column":17,"nodeType":"47","messageId":"48","endLine":16,"endColumn":25},"no-unused-vars","'setTodos' is assigned a value but never used.","Identifier","unusedVar"]